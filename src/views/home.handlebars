<div class="container-products">
    {{#each products}}
    <div class="container-product">
        <h3>{{title}}</h3>
        <p>trademark: {{trademark}}</p>
        <p>description: {{description}}</p>
        <p>price: {{price}}</p>
        <p>status: {{status}}</p>
        <p>category: {{category}}</p>
        <p>code: {{code}}</p>
        <p>stock: {{stock}}</p>
        <p><img src="{{thumbnail}}" alt="imagen" class="imagen"></p>
    </div>
    {{/each}}
</div>